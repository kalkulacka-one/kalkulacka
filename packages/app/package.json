{
  "name": "@kalkulacka-one/app",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./styles": "./dist/index.css",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "default": "./dist/client/index.js"
    }
  },
  "scripts": {
    "build": "npm run tailwind && tsc && tsc-alias",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "ncu": "ncu",
    "ncu:upgrade": "ncu -u",
    "tailwind": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "tailwind:watch": "npm run tailwind -- --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit"
  },
  "packageManager": "npm@11.5.2",
  "devDependencies": {
    "@kalkulacka-one/typescript-config": "*",
    "@tailwindcss/cli": "^4.1.17",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.19.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.8",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "react": "^19.2.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@kalkulacka-one/design-system": "*",
    "use-intl": "^4.3.9"
  },
  "peerDependencies": {
    "react": "^19.2.1"
  }
}
