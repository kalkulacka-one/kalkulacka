{
  "name": "schema-kalkulacka-one",
  "main": "index.js",
  "compatibility_date": "2025-02-15",
  "assets": {
    "directory": "./build",
    "binding": "ASSETS"
  },
  "build": {
    "command": "npm run build",
    "watch_dir": ["packages/schema/*", "apps/schema.kalkulacka.one/*"]
  },
  "routes": [
    {
      "pattern": "schema.kalkulacka.one",
      "custom_domain": true
    }
  ],
  "observability": {
    "enabled": true
  }
}
