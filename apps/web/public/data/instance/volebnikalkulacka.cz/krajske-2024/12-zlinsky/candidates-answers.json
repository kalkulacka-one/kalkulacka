  "220a7440-06a3-4fb1-ab60-09750e87f7e5": [
    {
      "questionId": "b2e48e5f-2a73-498a-8817-3f8c36663984",
      "answer": false,
      "comment": "Vzhledem k omezeným prostředkům kraje na školství je potřeba podporovat především učební obory"
    },
    {
      "questionId": "36079500-1556-4330-81c6-defe94472c99",
      "answer": null,
      "comment": "Není možné odpovědět jednoznačně, záleží na konkrétním případě."
    },
    {
      "questionId": "0908ae4c-c4bc-4908-8032-8bc96ae8ca49",
      "answer": false,
      "comment": "Budování obnovitelných zdrojů energie je státní politika. Kraj nemůže mrhat svými omezenými prostředky na podporu těchto aktivit."
    },
    {
      "questionId": "fcdf48ac-3831-4efe-a859-a7a34c446e1c",
      "answer": true,
      "comment": "Je to naše základní priorita. Chceme jako první předložit návrh zákona o zřízení vyšších územně samosprávných celků a nahradit 14 krajů 4 zeměmi - Čechy, Morava, Slezsko a Praha"
    },
    {
      "questionId": "a570227a-7275-46a9-9c45-2a34612749e4",
      "answer": null,
      "comment": "Nevíme jak si současná koalice představuje stávající politiku kraje."
    },
    {
      "questionId": "028b58e7-262d-47df-b7b0-677ab7924ac5",
      "answer": true,
      "comment": "Jen málo politiků má schopnost být dobrými manažery ve firmách."
    },
    {
      "questionId": "9106f935-3be1-45fc-9a27-9166923ae737",
      "answer": false,
      "comment": "Na to kraj už vůbec nemá prostředky"
    },
    {
      "questionId": "48ef1ae7-0a19-46b4-9281-b32e3c8a2c1d",
      "answer": true,
      "comment": "Vyrovnaný rozpočet by krajská zastupitelstva měla mít v rámci svého čtyřletého volebního období. V jednotlivých letech mohou mít rozpočty jak přebytkové, tak schodkové."
    },
    {
      "questionId": "f574fc72-052a-48d1-b6f5-34aa501bcc06",
      "answer": true,
      "comment": "Dotovat spoje kraj může pouze ve výjimečných případech"
    },
    {
      "questionId": "4111c840-9fb8-45a1-b055-724d14470ae8",
      "answer": false,
      "comment": "Kraj musí řešit jiné problémy než podporovat údržbu stromových alejí podél silnic"
    },
    {
      "questionId": "41d70ff6-dfc1-4d1e-92db-690a7da174b9",
      "answer": false,
      "comment": "Tuto problematiku by neměl řešit kraj, to by měla být záležitost obcí a měst, které by měly mít větší přehled o \"svých\" nízkopříjmových rodinách."
    },
    {
      "questionId": "a61bfce9-9699-431c-a8a4-c0ea52fb88cc",
      "answer": true,
      "comment": "To jsou hloupí kritici. Všestranný rozvoj studentů vede k tomu, že máme tisíce nepotřebných politologů, sociologů, analytiků, odborníků na genderová studia, pracovníků neziskovek apod., ale jen minimum potřebných a kvalitních konstruktérů a technologů."
    },
    {
      "questionId": "6fe97b31-10f2-442c-b854-edd8c1d42ec6",
      "answer": null,
      "comment": "Pokud kraj nemá víc disponibilních prostředků na tyto investice, tak jich víc provést nemůže."
    },
    {
      "questionId": "f3b18a48-6f14-4940-a559-2472a0ee3aa6",
      "answer": false,
      "comment": "Vysoké školství je v současnosti především v kompetenci státu, kraj by měl podporovat především střední školy technického zaměření a učňovské školství."
    },
    {
      "questionId": "4e5db964-0b90-4f67-a9d9-37f22e3de4fa",
      "answer": false,
      "comment": "Jednoznačně"
    },
    {
      "questionId": "8a03c517-3fe7-4165-bb80-ef631ee1727c",
      "answer": false,
      "comment": "Tuto problematiku si musí řešit školy sami. Menší školy mohou spolupracovat napřímo se školami, které s projektem mají větší zkušenosti."
    },
    {
      "questionId": "e9cf34e3-0386-4289-b64b-7bb09999ad1b",
      "answer": false,
      "comment": "Nemůžeme preferovat jeden přístup před druhým, záleží na konkrétních podmínkách a možnostech realizace a situaci dětí."
    },
    {
      "questionId": "de8044d9-ab63-4c74-aaff-3a2566b87f07",
      "answer": false,
      "comment": "Terénní služby pro své seniory by měly podporovat především města a obce, které znají nejlépe místní situaci."
    },
    {
      "questionId": "2c70783f-d5e2-4220-97ea-f86a7d046b27",
      "answer": false,
      "comment": "Opět by to měla být záležitost obcí a měst."
    },
    {
      "questionId": "a22e6904-7765-4426-93cb-2e1206cff30e",
      "answer": false,
      "comment": "Stát tyto neziskovky podporuje dostatečně, kraj tyto případné prostředky potřebuje využít v oblastech, kde není podpora státu a nebo je nedostatečná."
    },
    {
      "questionId": "5f8f2110-d09e-4647-86bf-27bd4552114d",
      "answer": false,
      "comment": "Na to kraj už vůbec nemá prostředky"
    },
    {
      "questionId": "bde5624a-7f8c-4d05-839e-655010417807",
      "answer": false,
      "comment": "Taková podpora nemůže zlepšit uplatitelnost výrazného počtu absolventů"
    },
    {
      "questionId": "0d3cf9cf-9320-4954-a99d-ea3ee975cc93",
      "answer": null
    },
    {
      "questionId": "18e71205-ad89-4a90-8741-c1b9a2727d1d",
      "answer": null,
      "comment": "To je naprosto individuální záležitost"
    },
    {
      "questionId": "369b7ba5-08d7-4531-be7b-28aabfdefda4",
      "answer": null,
      "comment": "Kompetence krajských zastupitelstev jsou naprosto zanedbatelné v kontextu s šíří společenského života, že řadový občan jejich činnost ani nezaregistruje"
    }
