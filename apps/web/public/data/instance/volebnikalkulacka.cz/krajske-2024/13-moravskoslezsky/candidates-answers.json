    {
      "questionId": "f3b18a48-6f14-4940-a559-2472a0ee3aa6",
      "answer": true
    },
    {
      "questionId": "4e5db964-0b90-4f67-a9d9-37f22e3de4fa",
      "answer": false,
      "comment": "Pojímat zdravotnictví jako byznys vede prokazatelně k jeho úpadku a nefunkčnosti."
